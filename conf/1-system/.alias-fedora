# alias for using dnf.
alias i='sudo dnf install'
alias ui='sudo dnf remove'
alias clean='sudo dnf clean'
alias updaats='sudo dnf update'

# ssh aliases
alias piet='ssh piet.local'
alias aad='ssh aad.local'
alias serge='ssh serge.local'
alias willem='ssh willem.local'
alias oscar='ssh oscar.local'

# sensors and stuff.
alias sens='sensors | grep CPUTIN'
